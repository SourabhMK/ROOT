{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "user-list-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/userList/UserListWebPart.js",
          "manifest": "./src/webparts/userList/UserListWebPart.manifest.json"
        }
      ]
    },
    "people-directory-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/peopleDirectory/PeopleDirectoryWebPart.js",
          "manifest": "./src/webparts/peopleDirectory/PeopleDirectoryWebPart.manifest.json"
        }
      ]
    },    
    "birthday-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/birthday/BirthdayWebPart.js",
          "manifest": "./src/webparts/birthday/BirthdayWebPart.manifest.json"
        }
      ]
    },
    "departmental-request-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/departmentalRequest/DepartmentalRequestWebPart.js",
          "manifest": "./src/webparts/departmentalRequest/DepartmentalRequestWebPart.manifest.json"
        }
      ]
    },
    "asset-reservation-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/assetReservation/AssetReservationWebPart.js",
          "manifest": "./src/webparts/assetReservation/AssetReservationWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "UserListWebPartStrings": "lib/webparts/userList/loc/{locale}.js",
    "PeopleDirectoryWebPartStrings": "lib/webparts/peopleDirectory/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "BirthdayWebPartStrings": "lib/webparts/birthday/loc/{locale}.js",
    "DepartmentalRequestWebPartStrings": "lib/webparts/departmentalRequest/loc/{locale}.js",
    "AssetReservationWebPartStrings": "lib/webparts/assetReservation/loc/{locale}.js"
  }
}
